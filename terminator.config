[global_config]
  title_hide_sizetext = True
  inactive_color_offset = 0.61
  suppress_multiple_term_dialog = True
[keybindings]
  cycle_next = <Primary><Alt>Tab
  cycle_prev = <Primary><Shift><Alt>Tab
  split_horiz = F9
  split_vert = F10
  next_tab = <Primary>Tab
  prev_tab = <Primary><Shift>Tab
  create_group = <Primary><Alt>g
  group_all = <Primary><Super>g
  group_all_toggle = <Super>g
  group_tab = <Primary><Super>t
  group_tab_toggle = <Super>t
  broadcast_off = <Primary>o
  broadcast_group = <Primary>g
  broadcast_all = <Primary>a
[profiles]
  [[default]]
    background_color = "#282a36"
    background_darkness = 0.9
    background_type = transparent
    cursor_color = "#f8f8f2"
    font = Fira Code weight=453 11
    foreground_color = "#f8f8f2"
    palette = "#262626:#e356a7:#42e66c:#e4f34a:#9b6bdf:#e64747:#75d7ec:#efa554:#7a7a7a:#ff79c6:#50fa7b:#f1fa8c:#bd93f9:#ff5555:#8be9fd:#ffb86c"
    use_system_font = False
    background_image = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]